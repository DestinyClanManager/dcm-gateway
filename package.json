    "express": "^4.14.0",
    "express-async-handler": "^1.0.4",
    "fast-sort": "^1.2.5",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.14",
    "morgan": "^1.7.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "rollbar": "^2.4.4"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
