    "fs-extra": "^2.0.0",
    "fs.extra": "^1.3.2",
    "jest": "^22.2.2",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "merge": ">=1.2.1",
    "nock": "^9.2.3",
    "nodemon": "^1.18.5",
    "regenerator-runtime": "^0.11.1",
