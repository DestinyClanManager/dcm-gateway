  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "dev": "nodemon --inspect --exec \"node -r dotenv/config -r babel-register\" src/index.js",
    "clean": "node_modules/.bin/rimraf dist",
    "prebuild": "npm run clean && node_modules/.bin/mkdirp dist",
    "build": "node_modules/.bin/babel src --out-dir dist --ignore spec.js",
    "postbuild": "cp ./package.json ./dist/package.json",
    "test": "jest --config jest.config.js --coverage",
    "lint": "./node_modules/.bin/esw src",
    "lint:watch": "esw -w src"
  },
  "engines": {
    "node": "8.9.4"
  },
  "keywords": [],
