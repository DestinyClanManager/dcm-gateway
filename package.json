{
  "name": "uofgx-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "node dist/index.js",
    "dev": "NODE_ENV=development nodemon src/index.js --exec \"node -r 'babel-register'\"",
    "clean": "node_modules/.bin/rimraf dist",
    "prebuild": "npm run clean && node_modules/.bin/mkdirp dist",
    "build": "node_modules/.bin/babel src --out-dir dist --ignore spec.js",
    "postbuild": "cp ./package.json ./dist/package.json && cp ./.env ./dist/.env",
    "test": "./node_modules/.bin/mocha test-configuration.js  \"./{,!(node_modules)/**/}*.spec.js\" --reporter list --require babel-polyfill",
    "test:watch": "npm run test -- --watch",
    "lint": "./node_modules/.bin/esw src",
    "lint:watch": "esw -w src"
  },
  "engines": {
    "node": "8.9.4"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.16.0",
    "cors": "^2.8.4",
    "dotenv": "^4.0.0",
    "express": "^4.14.0",
    "fast-sort": "^1.2.5",
    "morgan": "^1.7.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.2"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.1.1",
    "babel-polyfill": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "babel-register": "^6.22.0",
    "chai": "^3.5.0",
    "commander": "^2.9.0",
    "cpx": "^1.5.0",
    "eslint": "^3.14.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-watch": "^2.1.14",
    "fs-extra": "^2.0.0",
    "fs.extra": "^1.3.2",
    "mkdirp": "^0.5.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "open": "^0.0.5",
    "rimraf": "^2.5.4",
    "supertest": "^2.0.1",
    "testdouble": "^1.11.1"
  }
}
