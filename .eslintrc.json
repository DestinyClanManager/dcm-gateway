{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "arrowFunctions": true
    }
  },
  "env": {
    "node": true
  },
  "extends": ["standard"],
  "rules": {
    "generator-star-spacing": "off",
    "space-before-function-paren": ["error", "never"]
  }
}
